{
    "@context": {
      "foaf": "http://xmlns.com/foaf/0.1/",
      "ex": "http://example.org/"
    },
    "@graph": [
      {
        "@id": "ex:person1",
        "@type": "foaf:Person",
        "foaf:name": "John Doe",
        "foaf:mbox": "mailto:john.doe@example.org"
      },
      {
        "@id": "ex:person2",
        "@type": "foaf:Person",
        "foaf:name": "Jane Doe",
        "foaf:mbox": "mailto:jane.doe@example.org"
      },
      {
        "@id": "ex:organization",
        "@type": "foaf:Organization",
        "foaf:name": "Example Organization",
        "foaf:member": ["ex:person1", "ex:person2"]
      }
    ]
  }
  